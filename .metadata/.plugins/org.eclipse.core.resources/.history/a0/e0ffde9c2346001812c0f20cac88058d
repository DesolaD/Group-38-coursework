
import java.net.URL;
import java.util.ResourceBundle;

import javafx.fxml.FXML;
import javafx.fxml.Initializable;
import javafx.scene.control.Slider;
import javafx.scene.input.MouseEvent;

public class Warehouse implements Initializable {

	private Slider heightSlider;
	private Slider widthSlider;
	
	private int  hVALUE = 9;
	private int  vVALUE = 9;
	
	@Override
	public void initialize(URL arg0, ResourceBundle arg1) {
		heightSlider.setValue(hVALUE);
		widthSlider.setValue(vVALUE);
		// TODO Auto-generated method stub
		
	}
	
	public void sliderClick(MouseEvent event){
		System.out.println("njnj");
	}
}
